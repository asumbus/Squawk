[Unit]
Description=UDASHAVA
After=multi-user.target

[Service]
ExecStart=/usr/bin/python /home/pi/Desktop/GPS/run.py
StandardOutput=file:/home/pi/Desktop/GPS/logs/run1.log
StandardError=file:/home/pi/Desktop/GPS/logs/run2.log
Restart=always
RestartSec=10s
KillMode=process
TimeoutSec=infinity

[Install]
WantedBy=multi-user.target
